{
  "name": "eggs-generator",
  "version": "1.0.24",
  "description": "Tools for generating code snippets and optimization suggestions by HenFarm.js",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --config ../jest.config.cjs",
    "prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx,vue,json}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "coverage": "jest --coverage"
  },
  "jest": {
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(get-port)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "vue"
    ],
    "testMatch": [
      "**/tests/**/*.test.{js,jsx,ts,tsx}",
      "**/__tests__/**/*.{js,jsx,ts,tsx}",
      "**/backend/tests/**/*.test.{js,jsx,ts,tsx}"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx,vue}",
      "backend/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["html", "text", "lcov", "clover"],
    "globals": {
      "vue-jest": {
        "babelConfig": true
      },
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@backend/(.*)$": "<rootDir>/backend/$1"
    },
    "testPathIgnorePatterns": ["/node_modules/", "/dist/", "/coverage/"],
    "verbose": true,
    "bail": 1,
    "testTimeout": 30000,
    "maxWorkers": "50%"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "eslint": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "@vue/vue3-jest": "^29.2.6"
  },
  "dependencies": {
    "express": "^4.21.0",
    "lodash": "^4.17.21",
    "uuid": "^10.0.0"
  },
  "keywords": [
    "code-generation",
    "optimization",
    "dependency-management",
    "quality-assurance"
  ],
  "author": "Helloblue",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
