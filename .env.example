# API Configuration
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here
NODE_ENV=development

# AWS Configuration
AWS_PROFILE=your-sso-profile
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-bucket-name
AWS_LAMBDA_FUNCTION=your-lambda-function
AWS_SSO_START_URL=your-sso-start-url
AWS_SSO_REGION=your-sso-region
AWS_SSO_ACCOUNT_ID=your-account-id
AWS_SSO_ROLE_NAME=your-role-name

# Elasticsearch Configuration
ELASTICSEARCH_HOST=your-elasticsearch-host
ELASTICSEARCH_PORT=9200
ELASTICSEARCH_USERNAME=your-elasticsearch-username
ELASTICSEARCH_PASSWORD=your-elasticsearch-password
ELASTICSEARCH_INDEX=your-index-name
ELASTICSEARCH_SSL_VERIFY=true

# Server Configuration
PORT=3000
HOST=localhost

# Database Configuration
DB_HOST=localhost
DB_PORT=27017
DB_NAME=bleujs
DB_USER=bleujs_user
DB_PASSWORD=your_db_password_here

# AI Model Configuration
MODEL_PATH=./models
MAX_SEQUENCE_LENGTH=100
VOCABULARY_SIZE=10000
EMBEDDING_DIM=100
NUM_LAYERS=2
HIDDEN_UNITS=128
DROPOUT_RATE=0.2

# Security Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h
ENCRYPTION_KEY=your_encryption_key_here

# Monitoring Configuration
ENABLE_MONITORING=true
LOG_LEVEL=info
METRICS_PORT=9090

# Quantum Computing Configuration
QUANTUM_ENABLED=false
QUANTUM_SIMULATOR_URL=http://localhost:8080

# Cache Configuration
CACHE_TTL=3600
CACHE_ENABLED=true

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100

# Feature Flags
ENABLE_QUANTUM=true
ENABLE_AI=true
ENABLE_SECURITY=true
ENABLE_MONITORING=true
