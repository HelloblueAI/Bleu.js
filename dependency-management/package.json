{
  "name": "Bleujs-dependency-management",
  "version": "1.1.2",
  "type": "commonjs",
  "main": "src/index.js",
  "directories": {
    "src": "src",
    "test": "tests"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write 'src/**/*.js' 'tests/**/*.js'"
  },
  "keywords": [
    "dependency",
    "management",
    "bleujs",
    "package-manager",
    "version-control"
  ],
  "author": "Pejman Haghighatnia",
  "license": "MIT",
  "description": "Module for managing and resolving dependencies for Bleu.js.",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jest": "29.5.14",
    "@types/node": "^20.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.0.0",
    "express-winston": "^4.2.0",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.0.0",
    "node-notifier": "^10.0.1",
    "prettier": "^3.0.0",
    "ts-jest": "29.1.1",
    "typescript": "^5.7.3",
    "winston": "^3.17.0"
  }
}
