{
  "name": "bleujs-dependency-management",
  "version": "1.0.18",
  "type": "commonjs",
  "main": "src/index.js",
  "directories": {
    "src": "src",
    "test": "tests"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "build": "npm run lint && node src/index.js",
    "clean": "rm -rf node_modules && npm cache clean --force"
  },
  "keywords": [
    "dependency",
    "management",
    "bleujs",
    "npm",
    "module"
  ],
  "author": "Pejman Haghighatnia",
  "license": "MIT",
  "description": "Module for managing and resolving dependencies for Bleu.js.",
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.37.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
