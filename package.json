{
  "name": "bleujs",
  "version": "1.0.6",
  "description": "A production-ready AI and decision tree service",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:frontend": "http-server -p 4002 ./frontend/public",
    "start:backend": "node backend/server.js",
    "start:core-engine": "node core-engine/index.js",
    "start:eggs-generator": "node eggs-generator/src/index.js",
    "start:dependency-management": "node dependency-management/src/index.js",
    "build": "webpack --config frontend/webpack.config.cjs",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:backend": "jest --detectOpenHandles --testPathPattern=backend",
    "test:all": "jest --detectOpenHandles",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:frontend": "eslint frontend/**/*.js frontend/**/*.vue",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check .",
    "ci:lint": "eslint . --max-warnings=0",
    "ci:test": "pnpm run test:all",
    "ci:build": "pnpm run build",
    "ci:deploy": "echo 'Deploying...'",
    "ci:report": "jest --coverage --coverageReporters=text-lcov > coverage.lcov && codecov",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "preinstall": "npx npm-force-resolutions"
  },
  "dependencies": {
