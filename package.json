{
  "name": "bleujs",
  "version": "1.0.22",
  "packageManager": "pnpm@9.15.1",
  "description": "A production-ready AI and decision tree service for enterprise applications.",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "backend",
    "core-engine",
    "eggs-generator",
    "dependency-management",
    "language-plugins",
    "collaboration-tools"
  ],
  "bin": {
    "bleujs": "./bin/bleujs-cli.js"
  },
  "scripts": {
    "start": "node backend/server.ts",
    "build": "tsc",
    "run:operations": "node --loader ts-node/esm testMongoOperations.mts",
    "dev": "nodemon backend/server.js",
    "start:pm2": "pm2 start backend/server.js --name bleujs",
    "stop:pm2": "pm2 stop bleujs && pm2 delete bleujs",
    "test": "jest --logHeapUsage --detectOpenHandles --verbose",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=backend --coverage",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e --detectOpenHandles --runInBand",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "pnpm run lint:fix && pnpm run prettier:fix",
    "prettier:fix": "prettier --write \"{backend,core-engine}/**/*.{js,ts}\"",
    "prettier:check": "prettier --check \"{backend,core-engine}/**/*.{js,ts}\"",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "clean": "rimraf node_modules dist coverage pnpm-lock.yaml",
    "docker:build:backend": "docker build -f backend/Dockerfile -t bleujs-backend .",
    "docker:build:all": "docker-compose build",
    "docker:run:backend": "docker run --rm -p 3000:3000 bleujs-backend",
    "docker:run:all": "docker-compose up",
    "docker:clean": "docker system prune -f",
    "version:bump": "standard-version",
    "release": "pnpm run version:bump && git push --follow-tags origin main",
    "ci:build": "pnpm run clean && pnpm install && pnpm run lint && pnpm run test:all",
    "ci:docker": "pnpm run docker:build:all && pnpm run docker:run:all"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "compression": "^1.7.5",
    "decision-tree": "^0.3.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "get-port": "^7.1.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^6.12.0",
    "openai": "^4.77.0",
    "pm2": "^5.4.3",
    "portfinder": "^1.0.32",
    "rimraf": "^6.0.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-transform-object-rest-spread": "^7.25.9",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@cspell/cspell-types": "^8.17.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express-rate-limit": "^6.0.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "babel-jest": "^29.7.0",
    "core-js": "^3.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.11",
    "mongoose": "^8.9.2",
    "nyc": "^17.1.0",
    "prettier": "^3.4.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "pnpm": {
    "overrides": {
      "babel-traverse": "6.26.0",
      "glob": "^10.0.0",
      "@babel/plugin-proposal-object-rest-spread": "7.20.7",
      "@babel/plugin-proposal-optional-chaining": "^7.21.0"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{backend,core-engine}/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "ai",
    "decision-tree",
    "express",
    "mongodb",
    "nodejs",
    "jest",
    "eslint",
    "docker",
    "automation",
    "pm2",
    "lint-staged",
    "husky"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HelloblueAI/Bleu.js.git"
  },
  "bugs": {
    "url": "https://github.com/HelloblueAI/Bleu.js/issues"
  },
  "homepage": "https://github.com/HelloblueAI/Bleu.js#readme"
}
