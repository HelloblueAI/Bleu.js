{
  "name": "bleujs-frontend",
  "version": "1.0.15",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "webpack serve --config webpack.config.cjs --mode development",
    "build": "webpack --config webpack.config.cjs --mode production",
    "lint": "eslint 'src/**/*.{js,vue}' --cache",
    "lint:fix": "eslint 'src/**/*.{js,vue}' --cache --fix",
    "test": "jest",
    "prettier:fix": "prettier --write 'src/**/*.vue' 'src/**/*.js'"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "force": "^0.0.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
